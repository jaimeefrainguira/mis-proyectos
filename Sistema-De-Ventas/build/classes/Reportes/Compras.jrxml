	<queryString>
		<![CDATA[SELECT
CC.COM_CAB_ID,
PR.PER_NOMBRE AS PROVEEDOR,
US.PER_NOMBRE AS USUARIO,
CC.COM_CAB_FECHA,
CC.COM_CAB_TIPO_COMPROVANTE,
CC.COM_CAB_NUM_COMPROVANTE,
CC.COM_CAB_SUBTOTAL,
CC.COM_CAB_IVA,
CC.COM_CAB_TOTAL,
CC.COM_CAB_ESTADO,
CD.PRO_ID,
CD.COM_DET_CATIDAD,
P.PRO_NOMBRE,
CD.COM_DET_VALOR,
CD.COM_DET_TOTAL

FROM COMPRAS_CAB CC
JOIN COMPRAS_DETALLE CD ON CD.COM_CAB_ID=CC.COM_CAB_ID
JOIN PERSONA PR ON PR.PER_ID=CC.COM_CAB_PROVEEDOR_ID
JOIN PERSONA US ON US.PER_ID=CC.COM_CAB_USUARIO_ID
JOIN PRODUCTO P ON P.PRO_ID=CD.PRO_ID
WHERE CC.COM_CAB_ID=$P{COM_CAB_ID}]]>
	</queryString>
