	<queryString>
		<![CDATA[SELECT
     FACTURA_CAB.`FAC_CAB_ID` AS FACTURA_CAB_FAC_CAB_ID,
     FACTURA_CAB.`FAC_CAB_FECHA` AS FACTURA_CAB_FAC_CAB_FECHA,
     FACTURA_CAB.`FAC_CAB_SUBTOTAL` AS FACTURA_CAB_FAC_CAB_SUBTOTAL,
     FACTURA_CAB.`FAC_CAB_IVA` AS FACTURA_CAB_FAC_CAB_IVA,
     FACTURA_CAB.`FAC_CAB_TOTAL` AS FACTURA_CAB_FAC_CAB_TOTAL,
     FACTURA_CAB.`FAC_CAB_FORMA_P` AS FACTURA_CAB_FAC_CAB_FORMA_P,
     FACTURA_DETALLE.`FAC_DET_CANTIDAD` AS FACTURA_DETALLE_FAC_DET_CANTIDAD,
     FACTURA_DETALLE.`FAC_DET_PVP` AS FACTURA_DETALLE_FAC_DET_PVP,
     FACTURA_DETALLE.`FAC_DET_TOTAL` AS FACTURA_DETALLE_FAC_DET_TOTAL,
     PERSONA.`PER_CEDULA` AS PERSONA_PER_CEDULA,
     PERSONA.`PER_NOMBRE` AS PERSONA_PER_NOMBRE,
     PERSONA.`PER_DIRECCION` AS PERSONA_PER_DIRECCION,
     PERSONA.`PER_TELEFONO` AS PERSONA_PER_TELEFONO,
     PERSONA.`PER_CORREO` AS PERSONA_PER_CORREO,
     PRODUCTO.`PRO_DESCRIPCION` AS PRODUCTO_PRO_DESCRIPCION
FROM
     `FACTURA_CAB` FACTURA_CAB INNER JOIN `FACTURA_DETALLE` FACTURA_DETALLE ON FACTURA_CAB.`FAC_CAB_ID` = FACTURA_DETALLE.`FAC_CAB_ID`
     INNER JOIN `PERSONA` PERSONA ON FACTURA_CAB.`PER_ID` = PERSONA.`PER_ID`
     INNER JOIN `PRODUCTO` PRODUCTO ON FACTURA_DETALLE.`PRO_ID` = PRODUCTO.`PRO_ID`
	WHERE FACTURA_CAB.`FAC_CAB_ID`=$P{numeroFactura}]]>
	</queryString>
