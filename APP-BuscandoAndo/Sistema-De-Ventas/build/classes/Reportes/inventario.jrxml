	<queryString>
		<![CDATA[SELECT
     PRODUCTO.`PRO_ID` AS PRODUCTO_PRO_ID,
     PRODUCTO.`PRO_NOMBRE` AS PRODUCTO_PRO_NOMBRE,
     PRODUCTO.`PRO_DESCRIPCION` AS PRODUCTO_PRO_DESCRIPCION,
     PRODUCTO.`PRO_PRECIO_COSTO` AS PRODUCTO_PRO_PRECIO_COSTO,
     PRODUCTO.`PRO_PRECIO_VENTA` AS PRODUCTO_PRO_PRECIO_VENTA,
     PRODUCTO.`PRO_STOCK` AS PRODUCTO_PRO_STOCK,
     PRODUCTO.`PRO_STOCK_MINIMO` AS PRODUCTO_PRO_STOCK_MINIMO,
     PRODUCTO.`PRO_ESTADO` AS PRODUCTO_PRO_ESTADO,
     CATEGORIA.`CAT_DESCRIPCION` AS CATEGORIA_CAT_DESCRIPCION
FROM
     `PRODUCTO` PRODUCTO INNER JOIN `CATEGORIA` CATEGORIA ON PRODUCTO.`CAT_ID` = CATEGORIA.`CAT_ID`]]>
