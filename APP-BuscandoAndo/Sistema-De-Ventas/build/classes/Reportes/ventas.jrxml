	<queryString>
		<![CDATA[SELECT
    FC.FAC_CAB_ID,
    FC.FAC_CAB_FECHA,
    FC.FAC_CAB_SUBTOTAL,
    FC.FAC_CAB_IVA,
    FC.FAC_CAB_TOTAL,
    U.PER_NOMBRE AS USUARIO,
    C.PER_NOMBRE AS CLIENTE,
    C.PER_CEDULA,
    C.PER_DIRECCION,
    C.PER_TELEFONO,
    C.PER_CELULAR,
    C.PER_CORREO,
    FD.PRO_ID,
    FD.FAC_DET_CANTIDAD AS CANTIDAD,
    P.PRO_NOMBRE,
    FD.FAC_DET_PVP AS PRECIO,
    FD.FAC_DET_TOTAL AS TOTAL
FROM 
     FACTURA_CAB FC
     JOIN FACTURA_DETALLE FD ON FD.FAC_CAB_ID=FC.FAC_CAB_ID
     JOIN PERSONA U ON U.PER_ID=FC.USUARIO_ID
     JOIN PERSONA C ON C.PER_ID=FC.PER_ID
     JOIN PRODUCTO P ON P.PRO_ID=FD.PRO_ID
     AND FC.FAC_CAB_ID=$P{codigoCabesera}]]>
	</queryString>
